(defn pretty-print
  "Format doc strings into something org-mode will agree with."
  (x)
  (bind
   (sym (nth 0 x) docu (nth 1 x))
   (do
       (println "*** =" sym "=")
       (println "")
       (println "#+BEGIN_SRC elisp")
       (println docu)
       (println "#+END_SRC")
       (println ""))))

(foreach
 pretty-print
 (bind (all-syms (all-symbols))
       (zip all-syms (map doc all-syms))))
